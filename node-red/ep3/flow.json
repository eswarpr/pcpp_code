    {
        "id": "4bf1770ff7f80501",
        "type": "function",
        "z": "b358ac4e098d4b49",
        "name": "Calculate the active light",
        "func": "let current_light_index = flow.get(\"current_light_index\");\n\nif(flow.get(\"walk_button_state\") === 1 && current_light_index === 0) {\n    current_light_index = 1;\n    flow.set(\"walk_button_state\", 0);\n}\nelse if(current_light_index === 1) {\n    current_light_index = 0;\n}\n\nflow.set(\"current_light_index\", current_light_index);\n\nmsg.topic = \"lights/\" + flow.get(\"lights\")[current_light_index];\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "// Code added here will be run once\n// whenever the node is started.\nflow.set(\"lights\", [\"green\", \"red\"]);\nflow.set(\"current_light_index\", 0);",
        "finalize": "",
        "libs": [],
        "x": 370,
        "y": 200,
        "wires": [
            [
                "da681a2ff3ae3657"
            ]
        ]
    },
    {
